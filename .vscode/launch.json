  "version": "0.2.0",
  "configurations": [
    {
      "command": "dotnet run --project firstApp",
      "name": "run",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": ".NET Meteor Debugger",
      "type": "dotnet-meteor.debugger",
      "request": "launch",
      "preLaunchTask": "dotnet-meteor: Build"
    },
    {
      "name": "C#: firstApp Debug",
      "type": "dotnet",
      "request": "launch",
      "projectPath": "${workspaceFolder}/firstApp/firstApp.csproj"
    }
  ]
}
